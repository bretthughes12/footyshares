<%= semantic_form_for @match do |f| %>
  <%= f.inputs do %>
    <%= f.input :round, collection: Round.all %>
    <%= f.input :name %>
    <%= f.input :venue %>
    <%= f.input :allow_only_one_team %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
