<%= semantic_form_for(@team, :url => url, :html => { :method => method }) do |f| %>
  <%= f.inputs do %>
    <%= f.input :match, collection: Match.all %>
    <%= f.input :name %>
    <%= f.input :winner %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
